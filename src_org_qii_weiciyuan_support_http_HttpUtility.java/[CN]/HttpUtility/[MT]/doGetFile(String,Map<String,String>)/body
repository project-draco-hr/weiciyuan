{
  HttpResponse response=getDoGetHttpResponse(url,param);
  if (response != null) {
    String result=FileDownloaderHttpHelper.saveFile(url,response);
    if (response.getEntity() != null) {
      try {
        response.getEntity().consumeContent();
      }
 catch (      IOException e) {
        e.printStackTrace();
      }
    }
    return result;
  }
 else {
    return "";
  }
}
