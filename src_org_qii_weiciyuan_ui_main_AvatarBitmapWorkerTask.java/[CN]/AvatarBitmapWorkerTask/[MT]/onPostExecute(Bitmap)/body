{
  if (bitmap != null) {
    lruCache.put(data,bitmap);
    if (view != null && view.getTag().equals(data)) {
      view.setImageBitmap(bitmap);
      view.setTag("");
    }
  }
  if (taskMap != null && taskMap.get(getMemCacheKey(data,position)) != null) {
    taskMap.remove(getMemCacheKey(data,position));
  }
}
