{
  if (bitmap != null) {
    lruCache.put(data,bitmap);
  }
  if (taskMap != null && taskMap.get(data) != null) {
    taskMap.remove(data);
  }
  if (view != null) {
    ImageView imageView=view.get();
    if (imageView != null && imageView.getTag().equals(data)) {
      imageView.setTag("");
    }
  }
  super.onCancelled(bitmap);
}
