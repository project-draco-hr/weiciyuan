{
  if (urlKey.equals(view.getTag()) && view.getDrawable() != null && view.getDrawable() instanceof BitmapDrawable && ((BitmapDrawable)view.getDrawable() != null && ((BitmapDrawable)view.getDrawable()).getBitmap() != null)) {
    return false;
  }
 else {
    view.setTag(null);
    return true;
  }
}
