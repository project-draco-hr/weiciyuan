{
  haveGPS.setVisibility(View.VISIBLE);
  geoBean=new GeoBean();
  geoBean.setLatitude(result.getLatitude());
  geoBean.setLongitude(result.getLongitude());
  new GetGoogleLocationInfo().execute();
  ((LocationManager)StatusNewActivity.this.getSystemService(Context.LOCATION_SERVICE)).removeUpdates(locationListener);
}
