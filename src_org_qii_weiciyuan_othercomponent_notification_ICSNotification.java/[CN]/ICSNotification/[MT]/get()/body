{
  Notification.Builder builder=new Notification.Builder(context).setTicker(getTicker()).setContentTitle(getTicker()).setContentText(accountBean.getUsernick()).setSmallIcon(R.drawable.notification).setAutoCancel(true).setContentIntent(getPendingIntent()).setOnlyAlertOnce(true);
  if (getCount() > 1) {
    builder.setNumber(getCount());
  }
  Utility.configVibrateLedRingTone(builder);
  return builder.getNotification();
}
