{
  if (bitmap != null) {
    for (    WeakReference<ImageView> view : viewList) {
      if (view != null && view.get() != null) {
        if (canDisplay(view)) {
          playImageViewAnimation(view,bitmap);
          lruCache.put(url,bitmap);
        }
      }
    }
    if (taskMap != null && taskMap.get(url) != null) {
      taskMap.remove(url);
    }
  }
}
