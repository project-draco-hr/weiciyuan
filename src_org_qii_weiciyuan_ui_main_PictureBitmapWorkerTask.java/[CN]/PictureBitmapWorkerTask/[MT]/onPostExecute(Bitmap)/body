{
  if (bitmap != null) {
    lruCache.put(getMemCacheKey(data,position),bitmap);
    if (view != null) {
      ImageView imageView=view.get();
      if (imageView != null && imageView.getTag().equals(data) && position <= listView.getLastVisiblePosition() && position >= listView.getFirstVisiblePosition()) {
        imageView.setImageBitmap(bitmap);
      }
    }
  }
  if (taskMap.get(data) != null) {
    taskMap.remove(data);
  }
}
