{
  if (key.equals("enable_fetch_msg")) {
    boolean value=sharedPreferences.getBoolean("enable_fetch_msg",false);
    if (!value)     cancelAlarm();
  }
  if (key.equals("frequency")) {
    String value=sharedPreferences.getString(key,"1");
    if (value.equals("1"))     startAlarm(3 * 60 * 1000);
    if (value.equals("2"))     startAlarm(AlarmManager.INTERVAL_FIFTEEN_MINUTES);
    if (value.equals("3"))     startAlarm(AlarmManager.INTERVAL_HALF_HOUR);
  }
}
