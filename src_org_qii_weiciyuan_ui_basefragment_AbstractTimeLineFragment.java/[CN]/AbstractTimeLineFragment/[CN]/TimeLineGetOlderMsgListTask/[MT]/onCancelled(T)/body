{
  super.onCancelled(messageListBean);
  if (getActivity() != null) {
    if (this.e != null) {
      Toast.makeText(getActivity(),e.getError(),Toast.LENGTH_SHORT).show();
      showErrorFooterView();
      getPullToRefreshListView().onRefreshComplete();
    }
 else {
      dismissFooterView();
    }
  }
}
