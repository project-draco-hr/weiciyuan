{
  if (newTask != null && newTask.getStatus() != MyAsyncTask.Status.FINISHED) {
    return false;
  }
  if (oldTask != null && oldTask.getStatus() != MyAsyncTask.Status.FINISHED) {
    return false;
  }
  if (middleTask != null && middleTask.getStatus() != MyAsyncTask.Status.FINISHED) {
    return false;
  }
  return true;
}
