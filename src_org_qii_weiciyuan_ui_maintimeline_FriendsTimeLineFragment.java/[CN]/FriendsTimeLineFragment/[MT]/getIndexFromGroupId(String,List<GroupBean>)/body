{
  if (list == null || list.size() == 0) {
    return 0;
  }
  int index=0;
  if (id.equals("0")) {
    index=0;
  }
 else   if (id.equals("1")) {
    index=1;
  }
  for (int i=0; i < list.size(); i++) {
    if (list.get(i).getIdstr().equals(id)) {
      index=i + 2;
      break;
    }
  }
  return index;
}
