{
  super.onProgressUpdate(values);
  if (pb != null) {
    if (pb.getVisibility() != View.VISIBLE) {
      pb.setVisibility(View.VISIBLE);
    }
    if (!pbFlag) {
      pb.setIndeterminate(false);
      pbFlag=true;
    }
    Integer progress=values[0];
    Integer max=values[1];
    pb.setMax(max);
    pb.setProgress(progress);
  }
}
