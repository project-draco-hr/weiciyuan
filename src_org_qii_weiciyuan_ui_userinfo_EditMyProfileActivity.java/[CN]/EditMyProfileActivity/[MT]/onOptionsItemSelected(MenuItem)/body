{
  Intent intent;
switch (item.getItemId()) {
case android.R.id.home:
    intent=new Intent(this,MainTimeLineActivity.class);
  intent.addFlags(Intent.FLAG_ACTIVITY_CLEAR_TOP | Intent.FLAG_ACTIVITY_NEW_TASK);
startActivity(intent);
return true;
case R.id.menu_save:
save();
return true;
}
return false;
}
