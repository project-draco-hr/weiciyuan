{
  tasksResult.put(currentTask,true);
  boolean isAllTaskEnd=true;
  Set<UploadTask> taskSet=tasksResult.keySet();
  for (  UploadTask task : taskSet) {
    if (!tasksResult.get(task)) {
      isAllTaskEnd=false;
      break;
    }
  }
  if (isAllTaskEnd) {
    stopForeground(true);
    stopSelf();
  }
}
