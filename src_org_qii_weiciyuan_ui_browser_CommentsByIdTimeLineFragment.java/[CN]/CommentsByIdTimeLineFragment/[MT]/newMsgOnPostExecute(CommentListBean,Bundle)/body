{
  if (newValue != null && newValue.getSize() > 0) {
    getList().replaceAll(newValue);
    getAdapter().notifyDataSetChanged();
    getListView().setSelectionAfterHeaderView();
    invlidateTabText();
  }
 else   if (newValue != null && newValue.getSize() == 0) {
    getList().clear();
    invlidateTabText();
  }
}
