{
  File[] fileArray=file.listFiles();
  if (fileArray != null) {
    if (fileArray.length != 0) {
      for (      File fileSI : fileArray) {
        if (fileSI.isDirectory()) {
          handleDir(fileSI);
        }
        if (fileSI.isFile()) {
          handleFile(fileSI);
        }
      }
    }
 else {
      file.delete();
    }
  }
}
