{
  if (avatarBitmapWorkerTaskHashMap != null) {
    for (    String task : avatarBitmapWorkerTaskHashMap.keySet()) {
      avatarBitmapWorkerTaskHashMap.get(task).cancel(true);
    }
    avatarBitmapWorkerTaskHashMap=null;
  }
