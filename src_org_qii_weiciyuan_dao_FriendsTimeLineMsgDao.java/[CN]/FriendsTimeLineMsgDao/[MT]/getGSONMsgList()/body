{
  String json=getMsgListJson();
  Gson gson=new Gson();
  TimeLineMsgListBean value=null;
  try {
    value=gson.fromJson(json,TimeLineMsgListBean.class);
  }
 catch (  JsonSyntaxException e) {
    ActivityUtils.showTips("????????????????????????");
    AppLogger.e(e.getMessage().toString());
  }
  return value;
}
