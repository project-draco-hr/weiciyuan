{
  if (mVelocity != velocity) {
    mVelocity=velocity;
    if (mOnVelocityListViewListener != null) {
      mOnVelocityListViewListener.onVelocityChanged(velocity);
    }
    if (onVelocityEqualZeroListener != null && (mTime == INVALID_TIME)) {
      onVelocityEqualZeroListener.onZero();
    }
  }
}
