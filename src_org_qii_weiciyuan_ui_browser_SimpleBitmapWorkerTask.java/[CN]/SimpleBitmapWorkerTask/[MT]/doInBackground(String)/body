{
  data=url[0];
  if (!isCancelled()) {
switch (method) {
case picture_bmiddle:
      return ImageTool.getMiddlePictureInBrowserMSGActivity(data,null);
case avatar_small:
    return ImageTool.getSmallAvatarWithRoundedCorner(data);
case avatar_large:
  return ImageTool.getBigAvatarWithRoundedCorner(data);
}
}
return null;
}
