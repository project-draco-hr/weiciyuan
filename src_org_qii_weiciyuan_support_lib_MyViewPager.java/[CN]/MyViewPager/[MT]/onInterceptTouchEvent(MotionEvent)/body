{
  if (this.gestureDetector == null) {
    return super.onTouchEvent(ev);
  }
  if (isDragging) {
    return true;
  }
  if (operationItemPosition == 0) {
switch (ev.getActionMasked()) {
case MotionEvent.ACTION_MOVE:
      float x=ev.getRawX();
    if (x > firstPosition[0]) {
      return true;
    }
  break;
}
}
return super.onInterceptTouchEvent(ev);
}
