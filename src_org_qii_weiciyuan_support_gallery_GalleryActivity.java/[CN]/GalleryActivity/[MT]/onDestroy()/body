{
  super.onDestroy();
  for (  String url : urls) {
    MyAsyncTask task=taskMap.get(url);
    if (task != null)     task.cancel(true);
  }
  Utility.recycleViewGroupAndChildViews(pager,true);
  for (  ViewGroup viewGroup : views) {
    Utility.recycleViewGroupAndChildViews(viewGroup,true);
  }
  System.gc();
}
