{
  super.onCreate(savedInstanceState);
  ActionBar actionBar=getActionBar();
  actionBar.setDisplayHomeAsUpEnabled(true);
  actionBar.setTitle(getString(R.string.weibo));
  Intent intent=getIntent();
  token=intent.getStringExtra("token");
  msg=(MessageBean)intent.getSerializableExtra("msg");
  getSupportFragmentManager().beginTransaction().replace(android.R.id.content,new BrowserWeiboMsgFragment(msg)).commit();
}
