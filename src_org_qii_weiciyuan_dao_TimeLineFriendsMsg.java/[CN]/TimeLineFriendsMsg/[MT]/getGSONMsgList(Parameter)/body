{
  String json=getMsgs(param);
  Gson gson=new Gson();
  TimeLineMsgList value=null;
  try {
    value=gson.fromJson(json,TimeLineMsgList.class);
  }
 catch (  JsonSyntaxException e) {
    Log.e("gson","------------------------------");
    Log.e("gson",e.getMessage().toString());
  }
  return value;
}
